%h2 Dry run
%h4= task.name

= form_for :dry_run, routes.dry_runs_path(task_id: params[:task_id]), method: :post do
  - div class: 'form-group' do
    - label :url
    - url_field :url, class: 'form-control'

  - div class: 'form-group' do
    - submit 'Run', class: 'btn btn-primary'

- if result
  .jumbotron.jumbotron-fluid
    .container
      %p
        = result.data


